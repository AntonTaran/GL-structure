{
	"manifest_version": 2,
	"name": "Сертификация G Suite",
	"description": "Ссылка на веб-сайт сертификации G Suite",
	"version": "1.1",
	"icons": {
		"128": "128.png"
	},
	"app": {
		"urls": [
			"http://certification.googleapps.com/app-info/"
		],
		"launch": {
			"web_url": "http://certification.googleapps.com/"
		}
	},
	"permissions": [
		"unlimitedStorage",
		"notifications"
	]
}